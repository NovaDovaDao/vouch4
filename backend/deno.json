{
  "tasks": {
    "dev": "deno run -A main.ts",
    "prisma": "deno run -A npm:prisma",
    "prisma:generate": "deno task prisma generate --schema ./prisma/schema.prisma && deno task prisma:patch",
    "prisma:patch": "deno run -A ./prisma/patch.ts",
    "prisma:seed": "deno run -A --env ./prisma/seed.ts",
    "generate-token": "deno run -A --env jwt.ts",
    "codegen": "deno run -A npm:graphql-codegen",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "imports": {
    "@oak/oak": "jsr:@oak/oak@^17.1.6",
    "@std/assert": "jsr:@std/assert@1",
    "graphql-yoga": "npm:graphql-yoga@^3.7.3",
    "jose": "npm:jose@5.9.6",
    "better-auth": "npm:better-auth@^1.3.7",
    "@prisma/client": "npm:@prisma/client@^6.13.0",
    "prisma": "npm:prisma@^6.13.0",
    "dotenv-cli": "npm:dotenv-cli@^10.0.0",
    "graphql": "npm:graphql@^16.11.0",
    "graphql-scalars": "npm:graphql-scalars@^1.24.2"
  },
  "nodeModulesDir": "auto"
}